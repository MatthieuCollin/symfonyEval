<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
    <header>
        <div class="content">
            <img src="{{asset('build/img/285-401-max.png')}}" alt="logo">
            <h1>Éval symfony</h1>
        </div>
        <nav>
            <a href={{path('app_homepage')}}>acceuil</a>
            <a href={{path('app_products')}}>Produits</a>
            <a href={{path('app_post_produit_index')}}>Ajouter produit</a>
            <a href={{path('app_commentaire_index')}}>Gérer commentaire</a>
        </nav>
    </header>
        {% block body %}
        {% endblock %}
    </body>
</html>
